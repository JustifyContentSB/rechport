{
  "root": true,
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es2021": true
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "prettier"],
  "rules": {
    "no-unused-vars": "off",
    "no-console": "error",
    "no-prototype-builtins": "off"
  }
}
